{
  "name": "app-router_nexauth_prisma_stripe_sample",
  "version": "0.0.0",
  "description": "",
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "setup": "git config --local core.hooksPath .githooks && corepack enable pnpm",
    "dev": "pnpm dev:db:setup && next dev",
    "dev:db:setup": "pnpm db:start && pnpm dev:db:migrate && pnpm dev:db:generate",
    "dev:db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "dev:db:reset": "dotenv -e .env.local -- pnpm db:reset",
    "dev:db:studio": "dotenv -e .env.local -- prisma studio",
    "dev:db:generate": "dotenv -e .env.local -- pnpm generate:client",
    "db:start": "docker-compose up -d && pnpm wait",
    "db:stop": "docker-compose stop",
    "db:reset": "prisma migrate reset -f",
    "db:deploy": "prisma migrate deploy",
    "generate:client": "prisma generate",
    "wait": "sleep 0.5",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && biome lint .",
    "fmt": "prettier -w . && biome format --write .",
    "check": "biome check --apply ."
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "1.0.7",
    "@prisma/client": "5.13.0",
    "next": "14.2.3",
    "next-auth": "4.24.7",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "stripe": "14.25.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@types/node": "20.12.7",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.19",
    "dotenv-cli": "7.4.1",
    "lint-staged": "15.2.2",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "prisma": "5.13.0",
    "prisma-erd-generator": "1.11.2",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5"
  }
}
